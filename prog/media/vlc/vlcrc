# -*- mode: conf -*-

###
###  vlc 3.0.8
###

# see [[https://gist.github.com/mohkale/d159c8d1b80989f7c45f549e76278c18][here]] for the default configuration.

[qt] # Qt interface

# Resize interface to the native video size (boolean)
qt-video-autoresize=0

# Ask for network policy at start (boolean)
qt-privacy-ask=0

[core] # core program
# VoD server module (string)
vod-server=any

# Use only one instance when started from file manager (boolean)
one-instance-when-started-from-file=0

# Video snapshot file prefix (string)
snapshot-prefix=$N-$T-
