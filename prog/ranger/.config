# -*- mode: yaml -*-
- packages:
    pacman: ranger

- link:
    ~/.config/ranger/rc.conf: rangerrc
    ~/.config/ranger/commands.py:
    ~/.config/shortcuts/commands/ranger: shortcuts/ranger
    ~/.local/share/kservices5/ServiceMenus/open-ranger.desktop:
      if: echo "$DOTBOTS" | grep dolphin
      path: dolphin
