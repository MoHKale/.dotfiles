# -*- mode: yaml -*-
- link:
    ~/.config/shortcuts:
    ~/.ignore: ignore
    ~/.profile:
    ~/.config/shenv:
    ~/.config/dircolors:
    ~/.config/user-dirs.dirs: xdgdirs
    ~/.config/xdg: xdgenv
    ~/.config/bindings/readline: inputrc

- bots:
    - fonts
    - aliases
    - shells/bash
    - path: shells/zsh
      env:  true
